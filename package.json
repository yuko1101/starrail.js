{
  "name": "starrail.js",
  "version": "1.9.2",
  "description": "A Node.js Enka.Network API wrapper for Honkai: Star Rail.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "example": "example",
    "lib": "src",
    "doc": "docs"
  },
  "scripts": {
    "install": "node scripts/install.js",
    "lint": "npx eslint . --config eslint.config.mjs",
    "lint:fix": "npx eslint . --config eslint.config.mjs --fix",
    "build": "node scripts/clean.js && npx tsc",
    "build:debug": "node scripts/clean.js && npx tsc --sourceMap",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "starrail",
    "star rail",
    "honkai",
    "api",
    "mihoyo",
    "enka",
    "enka.network",
    "hoyoverse"
  ],
  "author": "yuko1101 (https://github.com/yuko1101)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuko1101/starrail.js.git"
  },
  "homepage": "https://starrail.vercel.app",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@types/json-bigint": "^1.0.4",
    "@types/node": "^22.0.0",
    "@types/unzip-stream": "^0.3.4",
    "@types/xxhashjs": "^0.2.4",
    "eslint": "^9.24.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "config_file.js": "^1.4.0",
    "enka-system": "^0.3.7",
    "json-bigint": "^1.0.0",
    "unzip-stream": "^0.3.4",
    "xxhashjs": "^0.2.2"
  }
}
